# Voice AI Professional System Environment Variables
# Copy this file to .env and fill in your actual values

# VAPI Configuration
VAPI_API_KEY= #your_vapi_api_key_here
VAPI_PROJECT_ID= #your_project_id_here

# OpenAI Configuration (for RAG)
OPENAI_API_KEY= #your_openai_api_key_here

# Pinecone Configuration (for vector storage)
PINECONE_API_KEY=#your_pinecone_api_key_here
PINECONE_ENVIRONMENT= us-east-1 #your_environment_here

# n8n Configuration
N8N_USER=admin
N8N_PASSWORD=#your_secure_password_here
N8N_ENCRYPTION_KEY=#your_32_character_encryption_key_here

# Database Configuration
POSTGRES_PASSWORD= #your_postgres_password_here

# Redis Configuration
REDIS_URL=redis://localhost:6379

# Memory System Configuration
MEMORY_TTL=86400
MAX_MEMORY_SIZE=1000
MEMORY_CLEANUP_INTERVAL=3600

# Professional Email Configuration
EMAIL_SMTP_HOST=smtp.gmail.com
EMAIL_SMTP_PORT=587
EMAIL_USERNAME=your-email@gmail.com
EMAIL_PASSWORD=your-app-password
EMAIL_USE_TLS=true
EMAIL_TEMPLATES_ENABLED=true

# Professional Timezone Configuration
DEFAULT_TIMEZONE=America/New_York
TIMEZONE_AUTO_DETECT=true
TIMEZONE_SUPPORTED_REGIONS=America,Europe,Asia,Australia

# Professional Callback Configuration
CALLBACK_ENABLED=true
CALLBACK_TYPES=email,sms,call,calendar
CALLBACK_PRIORITY_LEVELS=low,normal,high,urgent
CALLBACK_SCHEDULING_ENABLED=true

# Professional Sentiment Analysis
SENTIMENT_ANALYSIS_ENABLED=true
SENTIMENT_ESCALATION_THRESHOLD=0.3
SENTIMENT_REAL_TIME=true

# Professional Call Quality
CALL_QUALITY_MONITORING=true
CALL_RECORDING_ENABLED=true
CALL_TRANSCRIPTION_ENABLED=true
CALL_SENTIMENT_TRACKING=true

# Elasticsearch Configuration
ELASTICSEARCH_URL=http://localhost:9200

# Professional Monitoring Configuration
GRAFANA_PASSWORD=#your_grafana_password_here

# Professional Security Configuration
JWT_SECRET_KEY=#your_jwt_secret_key_here
ENCRYPTION_KEY=#your_encryption_key_here

# Professional SSL Configuration
SSL_ENABLED=true
SSL_CERT_PATH=/path/to/cert.pem
SSL_KEY_PATH=/path/to/key.pem

# Professional Logging Configuration
LOG_LEVEL=INFO
LOG_FORMAT=json
AUDIT_LOGGING_ENABLED=true
PERFORMANCE_LOGGING_ENABLED=true
ERROR_TRACKING_ENABLED=true

# Professional Performance Configuration
MAX_CONCURRENT_CALLS=50
MEMORY_LIMIT=2GB
CPU_LIMIT=4
LOAD_BALANCING_ENABLED=true
AUTO_SCALING_ENABLED=true
HEALTH_CHECKS_ENABLED=true

# Professional CRM Integration
GOHIGHLEVEL_API_KEY=your_ghl_api_key_here
GOHIGHLEVEL_LOCATION_ID=your_location_id_here
CRM_INTEGRATION_ENABLED=true

# Professional Template Configuration
EMAIL_TEMPLATE_CALL_SUMMARY=call_summary_template.html
EMAIL_TEMPLATE_FOLLOWUP=followup_template.html
EMAIL_TEMPLATE_APPOINTMENT=appointment_template.html

# Professional Alert Configuration
ALERT_HIGH_ERROR_RATE=0.05
ALERT_SLOW_RESPONSE_TIME=3.0
ALERT_HIGH_MEMORY_USAGE=0.8
ALERT_LOW_CALL_QUALITY=0.7

# Professional Data Retention
DATA_RETENTION_DAYS=30
AUDIT_LOG_RETENTION_DAYS=90
PERFORMANCE_LOG_RETENTION_DAYS=60 
